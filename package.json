{
  "name": "remark-video",
  "version": "0.6.3-beta.0",
  "description": "A remark plugin to parse video components",
  "type": "module",
  "exports": "./dist/index.js",
  "author": "brklntmhwk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brklntmhwk/remark-video"
  },
  "files": [
    "dist/*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "remark",
    "remark-plugin",
    "video",
    "html",
    "html5",
    "markdown"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "format": "bunx biome format ./src ./test",
    "format:fix": "bunx biome format --write ./src ./test",
    "lint": "bunx biome lint ./src ./test",
    "lint:fix": "bunx biome lint --write ./src ./test",
    "check": "bunx biome check ./src ./test",
    "check:fix": "bunx biome check --write ./src ./test",
    "test-type": "type-coverage"
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true,
    "ignoreCatch": true
  },
  "dependencies": {
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest",
    "@types/mdast": "^4.0.4",
    "@types/unist": "^3.0.3",
    "hast": "^1.0.0",
    "mdast-util-directive": "^3.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-directive": "^3.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "remark-stringify": "^11.0.0",
    "type-coverage": "^2.29.7",
    "typescript": "^5.6.3"
  }
}
